/* pages/test/test.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.circle-box {
  text-align: center;
  margin-top: 10vw;
  width: 10rpx;
  height: 10rpx;
  background: greenyellow;
}

.circle {
  position: absolute;
  background: gold;
}

.draw_btn {
  width: 35vw;
  position: absolute;
  top: 33vw;
  right: 0;
  left: 0;
  margin: auto;
  border: 1px #000 solid;
  border-radius: 5vw;
}
